<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yuan.springbootwebmybatis.mapper.UserMapper">
    <cache></cache>
    <insert id="insert">
        insert into webjpa.user (id, name)
        values (#{id,jdbcType=VARCHAR}, #{name,jdbcType=VARCHAR});
    </insert>
    <select id="selectAll" resultType="com.yuan.springbootwebmybatis.entity.User" useCache="true"
            statementType="PREPARED" flushCache="true">
        select id, name
        from webjpa.user
    </select>
</mapper>
